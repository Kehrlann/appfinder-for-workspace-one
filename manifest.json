{
  "name": "Chooser for VMware Workspace One",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Quickly open any app you have access to from Workspace One",
  "homepage_url": "http://github.com/mikfreedman/workspace-one-chooser",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_popup": "src/menu.html"
  },
  "background": {
    "scripts": ["src/bg/background.js"],
    "persistent": false
  },
  "permissions": [
    "cookies",
    "https://*.workspaceair.com/*"
  ],
  "omnibox": {
    "keyword": "w1"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}
